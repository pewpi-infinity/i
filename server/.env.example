# Environment variables for commit-server
# Copy this to .env and fill in your values

# GitHub personal access token (fine-grained)
# Required permissions: Contents (read & write) for repository pewpi-infinity/i
GITHUB_TOKEN=your_github_token_here

# Secret for authenticating commit requests
# Set this to a random string (e.g., generate with: openssl rand -hex 32)
COMMIT_SECRET=your_commit_secret_here

# Server port (optional, defaults to 3000)
PORT=3000
