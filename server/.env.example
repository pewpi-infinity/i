# GitHub Personal Access Token with repo scope
# Generate at: https://github.com/settings/tokens
GITHUB_TOKEN=ghp_your_token_here

# Shared secret for authenticating commit requests
# Set this to a random string (e.g., use openssl rand -hex 32)
COMMIT_SECRET=your_secret_here

# Port for the server (default: 3000)
PORT=3000

# GitHub repository details (default: pewpi-infinity/i)
GITHUB_OWNER=pewpi-infinity
GITHUB_REPO=i
GITHUB_LOG_PATH=logs/txt.log
# Copy to server/.env and edit with real values. DO NOT commit server/.env.
GITHUB_TOKEN=ghp_REPLACE_WITH_YOUR_TOKEN
COMMIT_SECRET=replace_with_a_random_string
PORT=4000
